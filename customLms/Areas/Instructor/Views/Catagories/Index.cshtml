@model IEnumerable<customLms.Entities.Catagory>
@using customLms.Areas.Instructor.Models;

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.Partial("_CreateButtonPartial")
</p>
<table class="table table-striped table-condensed">
    <tr class="success">
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        @Html.Partial("_TableButtonPartial", new SmallButtonModel { Id=item.id})
    </tr>
}

</table>
